{
  "Application": {
    "ApplicationTitle": "Local DNS"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ForwardedHeaders": {
    "Enabled": false
  },
  "Https": {
    "UseRedirectAndHsts": false
  },
  "Cors": {
    "Enabled": false,
    "PolicyName": "Default",
    "AllowedOrigins": [],
    "AllowAnyOrigin": false,
    "AllowedMethods": [],
    "AllowedHeaders": []
  },
  "Dnsmasq": {
    "MainConfigPath": "/etc/dnsmasq.conf",
    "ManagedFileName": "zz-dnsmasq-webui.conf",
    "ManagedHostsFileName": "zz-dnsmasq-webui.hosts",
    "SystemHostsPath": "/etc/hosts",
    "ReloadCommand": "systemctl reload dnsmasq",
    "StatusCommand": "systemctl is-active dnsmasq",
    "StatusShowCommand": "systemctl status dnsmasq --no-pager",
    "LogsCommand": "journalctl -u dnsmasq -n 100 --no-pager"
  }
}
