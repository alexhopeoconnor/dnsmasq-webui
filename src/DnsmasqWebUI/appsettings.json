{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Dnsmasq": {
    "HostsPath": "/etc/hosts",
    "ConfigPath": "/etc/dnsmasq.d/dhcp.conf",
    "LeasesPath": "/var/lib/misc/dnsmasq.leases",
    "ReloadCommand": "systemctl reload dnsmasq",
    "StatusCommand": "systemctl is-active dnsmasq"
  }
}
